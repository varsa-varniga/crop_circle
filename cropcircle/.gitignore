# Create a comprehensive .gitignore
cat > .gitignore << 'EOF'
# Google Earth Engine credentials
earth_engine_key.json
*earth_engine*
*earthengine*

# Service account keys
.serviceAccountKey.json
*.serviceAccountKey.json

# Environment variables
.env
.env.local
.env.development
.env.production

# Node modules
node_modules/
subsidies/node_modules/

# Logs
*.log
npm-debug.log*

# Uploaded files
uploads/

# Mac system files
.DS_Store

# Firebase
.firebase/
.firebaserc

# IDE files
.vscode/
.idea/

# Runtime data
pids
*.pid
*.seed
*.pid.lock
EOF